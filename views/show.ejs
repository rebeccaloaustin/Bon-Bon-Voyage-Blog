<%- include('partials/header') %>
    <body>
        <h1><%=blog.title; %></h1>
        <p><%=blog.content; %>.</p>
        <img src="<%=blog.img%>" />
        <p> Published: <%=blog.createdAt; %>.</p>
        <form class="text" action="/blog/<%=blog._id %>?_method=DELETE" method="POST">
            <input type = "submit" value="Delete">
        </form>
        <nav>
            <a class="text" href="/blog/<%=blog._id %>/edit">Edit article details</a>
        </nav>
        <nav>
            <a href="/blog">Back to blog</a>
        </nav>
    </body>
</html>
<%- include('partials/footer') %>